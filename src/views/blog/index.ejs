                      </li>
                      <li>
                        <a href="#">Video Tuts <span>(87)</span></a>
                      </li>
                    </ul>
                  </div>
                  <!-- end link-widget -->
                </div>
                <!-- end widget -->
              </div>
              <!-- end sidebar -->
            </div>
            <!-- end col -->
          </div>
          <!-- end row -->
        </div>
        <!-- end container -->
      </section>

<%- include('../includes/end.ejs') %>

